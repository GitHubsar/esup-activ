<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<!-- 
	/properties/jsf/navigation-rules.xml
	The configuration file of Java Server Faces that gives 
	the navigation rules between views.
	This file is referenced by /webapp/WEB-INF/web.xml. 
	It should not be modified but by the developers. 
-->

<faces-config>

	

	<navigation-rule>
		<display-name>index.jsp -> welcome.jsp</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administrators</display-name>
		<from-view-id>/stylesheets/administrators.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addAdmin</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteAdmin</from-outcome>
			<to-view-id>
				/stylesheets/administratorDelete.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administratorDelete</display-name>
		<from-view-id>
			/stylesheets/administratorDelete.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>adminDeleted</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administratorAdd</display-name>
		<from-view-id>/stylesheets/administratorAdd.jsp</from-view-id>
		<navigation-case>
			<from-outcome>adminAdded</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<display-name>accountWelcomeToIdentification</display-name>
		<from-view-id>/stylesheets/welcome.jsp</from-view-id>
		<navigation-case>
			<from-outcome>goToInfoToValidate</from-outcome>
			<to-view-id>/stylesheets/accountIdentification.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountIdentificationToPersonalInfo</display-name>
		<from-view-id>/stylesheets/accountIdentification.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPersonalInfo</from-outcome>
			<to-view-id>/stylesheets/accountPersonalInfo.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountIdentificationToCode</display-name>
		<from-view-id>/stylesheets/accountIdentification.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPushCode</from-outcome>
			<to-view-id>/stylesheets/accountCode.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountIdentificationToChoice</display-name>
		<from-view-id>/stylesheets/accountIdentification.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoChoice</from-outcome>
			<to-view-id>/stylesheets/accountChoice.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountChoiceToCode</display-name>
		<from-view-id>/stylesheets/accountChoice.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPushCode</from-outcome>
			<to-view-id>/stylesheets/accountCode.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountPersonalInfoToCharter</display-name>
		<from-view-id>/stylesheets/accountPersonalInfo.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoCharterAgreement</from-outcome>
			<to-view-id>/stylesheets/accountCharter.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	
	<navigation-rule>
		<display-name>accountPersonalInfoToPassword</display-name>
		<from-view-id>/stylesheets/accountPersonalInfo.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPasswordChange</from-outcome>
			<to-view-id>/stylesheets/accountPassword.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountPersonnalInfoToLogin</display-name>
		<from-view-id>/stylesheets/accountPersonalInfo.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoLoginChange</from-outcome>
			<to-view-id>/stylesheets/accountLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		
		
	
	<navigation-rule>
		<display-name>accountCharterToPassword</display-name>
		<from-view-id>/stylesheets/accountCharter.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPasswordChange</from-outcome>
			<to-view-id>/stylesheets/accountPassword.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountCodeToPersonalInfo</display-name>
		<from-view-id>/stylesheets/accountCode.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPersonalInfo</from-outcome>
			<to-view-id>/stylesheets/accountPersonalInfo.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountWelcomeToAuthentification</display-name>
		<from-view-id>/stylesheets/welcome.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoAuthentification</from-outcome>
			<to-view-id>/stylesheets/accountAuthentification.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountAuthentificationToPersonalInfo</display-name>
		<from-view-id>/stylesheets/accountAuthentification.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoPersonalInfo</from-outcome>
			<to-view-id>/stylesheets/accountPersonalInfo.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountAuthentificationToDataChange</display-name>
		<from-view-id>/stylesheets/accountAuthentification.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoDataChange</from-outcome>
			<to-view-id>/stylesheets/accountDataChange.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountDataChangeToShowDataChange</display-name>
		<from-view-id>/stylesheets/accountDataChange.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoViewDataChange</from-outcome>
			<to-view-id>/stylesheets/accountViewDataChange.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountLoginToEnabled</display-name>
		<from-view-id>/stylesheets/accountLogin.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoAccountEnabled</from-outcome>
			<to-view-id>/stylesheets/accountEnabled.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>accountPasswordToEnabled</display-name>
		<from-view-id>/stylesheets/accountPassword.jsp</from-view-id>
		<navigation-case>
			<from-outcome>gotoAccountEnabled</from-outcome>
			<to-view-id>/stylesheets/accountEnabled.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<display-name>ldapSearch</display-name>
		<from-view-id>/stylesheets/ldapSearch.jsp</from-view-id>
		<navigation-case>
			<from-outcome>cancelToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToDepartmentManagerAdd</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerAdd.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ldapResults</display-name>
		<from-view-id>/stylesheets/ldapResults.jsp</from-view-id>
		<navigation-case>
			<from-outcome>userSelectedToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>applicationRestarted</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationWelcome</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationAdministrators</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationPreferences</from-outcome>
			<to-view-id>/stylesheets/preferences.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationAbout</from-outcome>
			<to-view-id>/stylesheets/about.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>casLogin</from-outcome>
			<to-view-id>/stylesheets/cas.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ldapSearch</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ldapChoose</from-outcome>
			<to-view-id>/stylesheets/ldapResults.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	

</faces-config>
