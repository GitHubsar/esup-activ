<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean
		id="beanActivation"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="PASSWORD.PASSWORDCHANGE.TITLE"/>
		<property name="value" value="${view.procedure.activation}"/>
		
	</bean>
	
	<bean
		id="beanReinitialisation"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="WELCOME.PROCEDURE.REINITIALISATION.RADIOBUTTON.LABEL"/>
		<property name="value" value="${view.procedure.reinitialisation}"/>
		
	</bean>
	
	
	<bean
		id="beanPasswordChange"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="PASSWORD.PASSWORDCHANGE.TITLE"/>
		<property name="value" value="${view.procedure.passwordchange}"/>
		
	</bean>
	
	<bean
		id="beanLoginChange"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="WELCOME.PROCEDURE.LOGINCHANGE.RADIOBUTTON.LABEL"/>
		<property name="value" value="${view.procedure.loginchange}"/>
		
	</bean>

</beans>
