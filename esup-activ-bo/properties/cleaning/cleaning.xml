<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean
		id="cleaningValidationCode"
		class="org.esupportail.activbo.domain.tools.CleaningValidationCode"
		>
		
		<property name="formatDateConv" value="${validation.date.format}"/>
		<property name="validationCodeDateKey" value="${validation.date.key}"/>
		<property name="cleaningTimeInterval" value="${access.cleaning.timeinterval}"/>
		<property name="validationCode" ref="validationCode"/>
		
	</bean>
	
	

</beans>
