<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean
		id="cleaningHashCode"
		class="org.esupportail.activbo.domain.tools.CleaningHashCode"
		>
		
		<property name="formatDateConv" value="${access.date.format}"/>
		<property name="hashCodeDateKey" value="${access.date.key}"/>
		<property name="hashCodeCodeKey" value="${access.code.key}"/>
		<property name="hashCode" ref="hashCode"/>
		<property name="cleaningTimeInterval" value="${access.cleaning.timeinterval}"/>
		
	</bean>
	
	

</beans>
